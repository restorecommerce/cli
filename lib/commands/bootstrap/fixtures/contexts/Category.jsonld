{
  "@context": {
    "rstc": "http://restorecommerce.io/",
    "rdf": "http://www.w3.org/ns/org#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "skos": "http://www.w3.org/2004/02/skos/core#",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "dct": "http://purl.org/dc/terms/",
    "schema": "http://schema.org/",
    "slug": { "@id" : "rstc:classes/Category#slug", "@container" : "@language"},
    "name": { "@id": "rdfs:label", "@container": "@language"},
    "description": { "@id" : "dct:description", "@container" : "@language"},
    "detailedDesc": { "@id": "rstc:classes/Category#detailedDesc", "@type": "@id" },
    "displayOrder": "rstc:classes/Category#displayOrder",
    "active": "rstc:classes/Category#active",
    "broader": { "@id": "skos:broaderTransitive", "@type": "@id" },
    "prodTaxo": { "@id": "schema:category", "@type": "@id" },
    "created": "dct:created",
    "modified": "dct:modified",
    "creator": { "@id": "dct:creator", "@type": "@id" }
  }
}

