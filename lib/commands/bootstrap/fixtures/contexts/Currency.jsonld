{
  "@context": {
    "rstc": "http://restorecommerce.io/",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "dct": "http://purl.org/dc/terms/",
    "skos": "http://www.w3.org/2004/02/skos/core#",
    "dbo": "http://dbpedia.org/ontology/",
    "gn": "http://www.geonames.org/ontology#",
    "name": {
      "@id": "rdfs:label",
      "@container": "@language"
    },
    "shortName": "skos:altLabel",
    "code": "dbo:currencyCode",
    "symbol": "rstc:classes/Currency#symbol",
    "minorExponent": "rstc:classes/Currency#minorExponent",
    "minorName": "rstc:classes/Currency#minorName",
    "currencyOf": { "@id": "rstc:classes/Currency#currencyOf", "@container": "@set" },
    "created": "dct:created",
    "modified": "dct:modified"
  }
}
