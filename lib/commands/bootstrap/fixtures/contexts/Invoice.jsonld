{
  "@context": {
    "rstc": "http://restorecommerce.io/",
    "dct": "http://purl.org/dc/terms/",
    "schema": "http://schema.org/",
    "invoice": "rstc:classes/Invoice#",
    "order": { "@id": "schema:referencesOrder", "@type": "@id"},
    "customer": { "@id": "schema:customer", "@type": "@id"},
    "paymentDue": "schema:paymentDue",
    "number": "invoice:number",
    "applicableTax": {
      "@id": "rstc:classes/Invoice#applicableTax",
      "@type": "@id"
    },
    "price": "invoice:price",
    "priceNoVat": "invoice:priceNoVat",
    "priceVat": "invoice:priceVat",
    "total": "invoice:total",
    "totalNoVat": "invoice:totalNoVat",
    "totalVat": "invoice:totalVat",
    "created": "dct:created",
    "modified": "dct:modified",
    "creator": { "@id": "dct:creator", "@type": "@id" },
    "items": {
      "@id": "invoice:items",
      "@container": "@set"
    },
    "offer": { "@id": "rstc:classes/Offer", "@type": "@id" },
    "quantity": "invoice:quantity",
    "comment": "schema:comment",
    "configuration": {
      "@id": "cart:configuration",
      "@container": "@set"
    },
    "productConfigurationAttribute": {
      "@type": "@id",
      "@id": "rstc:classes/Product#productConfigurationAttribute"
    },
    "optionId": "rstc:classes/Product_Configuration_Attribute#optionId",
    "vouchers": {
      "@id": "rstc:classes/Invoice#vouchers",
      "@container": "@set"
    },
    "id": "rstc:classes/Invoice#id",
    "value": "rstc:classes/Invoice#value",
    "sent": "rstc:classes/Invoice#sent",
    "actualDiscount": "rstc:classes/Invoice#actualDiscount",
    "discountScheme": "rstc:classes/Invoice#discountScheme",
    "discount": "rstc:classes/Invoice#discount"
  }
}
