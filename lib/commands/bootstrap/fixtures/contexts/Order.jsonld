{
  "@context": {
    "rstc": "http://restorecommerce.io/",
    "rdf": "http://www.w3.org/ns/org#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "dct": "http://purl.org/dc/terms/",
    "schema": "http://schema.org/",
    "total": "rstc:classes/Order#total",
    "cart": {
      "@type": "@id",
      "@id": "rstc:classes/Cart"
    },
    "person": {
      "@type": "@id",
      "@id": "rstc:classes/Person"
    },
    "acceptedOffer": {
      "@type": "@id",
      "@id": "rstc:classes/Offer"
    },
    "billingAddress": {
      "@type": "@id",
      "@id": "rstc:classes/Order#billingAddress"
    },
    "deliveryAddress": {
      "@type": "@id",
      "@id": "rstc:classes/Order#deliveryAddress"
    },
    "deliveryChargeSpec": {
      "@id": "rstc:classes/Delivery_Charge_Specification",
      "@type": "@id"
    },
    "confirmationNumber": "schema:confirmationNumber",
    "customer": {
      "@type": "@id",
      "@id": "rstc:classes/Person"
    },
    "discount": "schema:discount",
    "discountCode": "schema:discountCode",
    "discountCurrency": {
      "@type": "@id",
      "@id": "rstc:classes/Currency"
    },
    "isGift": "schema:isGift",
    "merchant": "schema:merchant",
    "orderDate": "schema:orderDate",
    "orderNumber": "schema:orderNumber",
    "orderStatus": "schema:orderStatus",
    "orderedItem": {
      "@type": "@id",
      "@id": "rstc:classes/Product"
    },
    "paymentDue": "schema:paymentDue",
    "paymentMethod": {
      "@type": "@id",
      "@id": "rstc:classes/Payment_Method"
    },
    "paymentMethodId": "schema:paymentMethodId",
    "additionalType": "schema:additionalType",
    "alternateName": "schema:alternateName",
    "comment": "schema:comment",
    "description": {
      "@id": "dct:description",
      "@container": "@language"
    },
    "image": "schema:image",
    "name": {
      "@id": "rdfs:label",
      "@container": "@language"
    },
    "sameAs": "schema:sameAs",
    "created": "dct:created",
    "modified": "dct:modified",
    "creator": {
      "@id": "dct:creator",
      "@type": "@id"
    },
    "invoice": {
      "@type": "@id",
      "@id": "rstc:classes/Invoice"
    }
  }
}
