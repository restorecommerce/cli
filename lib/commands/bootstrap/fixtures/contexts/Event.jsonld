{
  "@context": {
    "rstc": "http://restorecommerce.io/",
    "dct": "http://purl.org/dc/terms/",
    "schema": "http://schema.org/",
    "name": { "@id": "schema:name", "@container": "@language" },
    "active": "rstc:classes/Event#active",
    "description": { "@id": "dct:description", "@container": "@language" },
    "detailedDesc": { "@id": "rstc:classes/Event#detailedDesc", "@type": "@id" },
    "image": { "@id": "schema:image", "@type": "@id"},
    "url": { "@id": "schema:url", "@type": "@id"},
    "location": "schema:location",
    "startDate": "schema:startDate",
    "endDate": "schema:endDate",
    "created": "dct:created",
    "modified": "dct:modified",
    "specification": { "@id": "rstc:classes/Event#specification", "@type": "@id" },
    "participants": "rstc:classes/Event#participants",
    "virtual": "rstc:classes/Event#virtual",
    "applicableTax": {
      "@id": "rstc:classes/Offer#applicableTax",
      "@type": "@id"
    },
    "schedule": {
      "@id": "rstc:classes/Offer#schedule",
      "@type": "@id"
    },
    "creator": { "@id": "dct:creator", "@type": "@id" },
    "preReservations": "rstc:classes/Event#preReservations",
    "reservedForToken": "rstc:classes/Event#reservedForToken",
    "reservedUntil": "rstc:classes/Event#reservedUntil",
    "reservation": { "@id": "rstc:classes/Event#reservation", "@type": "@id", "@container": "@set" },
    "inventoryLevel": "rstc:classes/Event#inventoryLevel",
    "eventStatus": "schema:eventStatus",
    "numberOfParticipants": "rstc:classes/Event#numberOfParticipants",
    "prototype": { "@id": "rstc:classes/Product#prototype", "@type": "@id"}
  }
}
