{
  "@context": {
    "rstc": "http://restorecommerce.io/",
    "rdf": "http://www.w3.org/ns/org#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "dct": "http://purl.org/dc/terms/",
    "schema": "http://schema.org/",
    "appliesToDeliveryMethod": { "@id": "rstc:classes/Delivery_Method", "@type": "@id" },
    "eligibleRegion": { "@type": "@id", "@id": "rstc:classes/Country" },
    "eligibleQuantity": "schema:eligibleQuantity",
    "maxValue": "schema:maxValue",
    "minValue": "schema:minValue",
    "unitCode": "schema:unitCode",
    "value": "schema:value",
    "eligibleTransactionVolume": "schema:eligibleTransactionVolume",
    "maxPrice": "schema:maxPrice",
    "minPrice": "schema:minPrice",
    "price": "schema:price",
    "priceCurrency": { "@type": "@id", "@id": "rstc:classes/Currency" },
    "validFrom": "schema:validFrom",
    "validThrough": "schema:validThrough",
    "valueAddedTaxIncluded": "schema:valueAddedTaxIncluded",
    "additionalType": "schema:additionalType",
    "alternateName": "schema:alternateName",
    "description": { "@id": "dct:description", "@container": "@language" },
    "image": "schema:image",
    "name": { "@id": "rdfs:label", "@container": "@language" },
    "sameAs": "schema:sameAs",
    "created": "dct:created",
    "modified": "dct:modified",
    "creator": { "@id": "dct:creator", "@type": "@id" },
    "displayOrder": "rstc:classes/Delivery_Charge_Specification#displayOrder"
  }
}
